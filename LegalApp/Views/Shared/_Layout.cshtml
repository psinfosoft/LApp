    <script type="text/javascript">
        function notify(message, type) {
            var xoffSet;
            var alignment;
            if (message.length >= 60) {
                xoffSet = 350;
                alignment = "left";
            }
            else {
                xoffSet = 0;
                alignment = "center";
            }
            $.growl({
                message: message
            }, {
                type: type,
                allow_dismiss: true,
                label: 'Cancel',
                className: 'btn-xs btn-inverse',
                placement: {
                    from: 'top',
                    align: alignment
                },
                offset: {
                    x: xoffSet,
                    y: 50
                },
                delay: 5000,
                animate: {
                    enter: 'animated bounceIn',
                    exit: 'animated bounceOut'
                }
            });
        }
    </script>
